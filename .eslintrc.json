{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:json/recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/no-duplicates": 0,
        "eol-last": [
            "error",
            "never"
        ],
        "semi": [
            1,
            "always"
        ],
        "no-unused-vars": 0
    }
}